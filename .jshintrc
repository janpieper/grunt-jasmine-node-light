  "camelcase": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "quotmark": true,
  "undef": true,
  "unused": true,
  "globalstrict": true,
